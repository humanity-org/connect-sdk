HUMANITY_SDK_CLIENT_ID=local-client-id
HUMANITY_SDK_CLIENT_SECRET=local-client-secret
HUMANITY_SDK_REDIRECT_URI=http://localhost:3000/oauth/callback
HUMANITY_SDK_BASE_URL=http://localhost:3000
HUMANITY_SDK_ENVIRONMENT=local

# Preset / scope configuration
HUMANITY_SDK_TEST_PRESET_KEYS=isHuman,is18Plus
HUMANITY_SDK_TEST_SCOPES=
HUMANITY_SDK_TEST_UNAVAILABLE_PRESET=

# Wallet-issued shared token the suite uses to approve scopes
HUMANITY_SDK_TEST_SHARED_TOKEN=eyJhbGciOiJSUzI1NiIsInR5cCI6IkpXVCJ9...

# Optional knobs
HUMANITY_SDK_TEST_STATE=integration-state
HUMANITY_SDK_TEST_UPDATED_SINCE=
HUMANITY_SDK_TEST_AUTHORIZATION_STATUS=active
HUMANITY_SDK_TEST_TIMEOUT_MS=60000
HUMANITY_SDK_TEST_DELAY_MS=200